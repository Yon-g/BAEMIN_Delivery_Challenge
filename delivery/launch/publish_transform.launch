<launch>
    <!-- map -> odom -->
    <node pkg="tf" type="static_transform_publisher" name="map_to_odom" args="0 0 0 0 0 0 map odom 100" />

    <!-- odom -> base_link -->
    <node pkg="tf" type="static_transform_publisher" name="odom_to_base_link" args="0 0 0 0 0 0 odom base_link 100" />

    <!-- base_link -> velodyne -->
    <node pkg="tf" type="static_transform_publisher" name="base_link_to_velodyne" args="0 0 0.73 0 0 0 base_link velodyne 100" />
</launch>
